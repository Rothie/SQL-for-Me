-- SQL PROJECT
-- AMAZON GIFT CARD PROJECT

CREATE TABLE sales (

Company_Name VARCHAR(100) NOT NULL PRIMARY KEY,
State VARCHAR(2) DEFAULT 'NA',
Account_fname VARCHAR(100) DEFAULT 'US_Sales',
Account_lname VARCHAR(100) DEFAULT 'US_Sales',
Order_type VARCHAR(100)  DEFAULT 'NA',
Order_date DATE,
Order_amount FLOAT(10,2),
Contact_Per VARCHAR(100)
);

